@model CreateFooterAddressDto
@{
    ViewData["Title"] = "CreateFooterAddress";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}



<div class="page-content">
    <div class="container-fluid">

        <!-- start page title -->
        <div class="row">
            <div class="col-xl-12">
                <div class="card">
                    <div class="card-body">

                        <h4 class="card-title">Yeni Footer Address Girişi</h4>
                        <p class="card-subtitle mb-4">FooterAddress ile ilgili temel özellikleri eksiksiz doldurmanız önemlidir.</p>

                        <form method="post">



                            <div class="form-group">
                                <label for="exampleInputDescription">Açıklama</label>
                                <input type="text" class="form-control" id="exampleInputDescription" asp-for="Description" placeholder="Açıklama Girişi Yapınız">
                            </div>

                            <div class="form-group">
                                <label for="exampleInputAddress">Adres</label>
                                <input type="text" class="form-control" id="exampleInputAddress" asp-for="Address" placeholder="Adres Girişi Yapınız">
                            </div>

                            <div class="form-group">
                                <label for="exampleInputPhone">Telefon</label>
                                <input type="text" class="form-control" id="exampleInputPhone" asp-for="Phone" placeholder="Telefon Numarası Girişi Yapınız">
                            </div>

                            <div class="form-group">
                                <label for="exampleInputEmail">Email</label>
                                <input type="text" class="form-control" id="exampleInputEmail" asp-for="Email" placeholder="Email Girişi Yapınız">
                            </div>




                            <button type="submit" class="btn btn-primary waves-effect waves-light">Yazar Kaydet</button>
                        </form>

                    </div> <!-- end card-body-->
                </div>
                <!-- end card -->

            </div>

        </div>
        <!-- end page title -->

    </div> <!-- container-fluid -->
</div>
